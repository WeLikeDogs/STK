local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()
 
local w1 = library:Window("STK Auto Farm BETA") -- Text
 
w1:Button(
    "(1) SendKeyEvent",
    function()
        while true do
    wait(0.8)
    game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
    end
    end
) -- Text, Callback

w1:Button(
    "(2) Start Auto (Permanent)",
    function()
while true do
    wait(1)
for i, p in pairs(game.workspace:GetChildren()) do
if p:FindFirstChild("LootSpawns") then
local lootSpawns = p.LootSpawns
local player = game:GetService("Players").LocalPlayer
local count = 0

for _, lootSpawn in pairs(lootSpawns:GetChildren()) do
  player.Character.HumanoidRootPart.CFrame = lootSpawn.CFrame
  count = count + 1
  if count == 8 then
      wait(1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
wait(1)
end
end
end
end
end
) -- Text, Callback
 
w1:Button(
    "Destroy GUI",
    function()
        for i, v in pairs(game.CoreGui:GetChildren()) do
            if v:FindFirstChild("Top") then
                v:Destroy()
            end
        end
    end
) -- Text, Callback
 
w1:Label("üêï We Likes Dogs üêï") -- TextÔá±
